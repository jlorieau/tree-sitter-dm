{
  "name": "disseminate",
  "rules": {
    "document": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "p"
      }
    },
    "p": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "REPEAT1",
              "content": {
                "type": "SYMBOL",
                "name": "_node"
              }
            },
            {
              "type": "SYMBOL",
              "name": "_new_block"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "REPEAT1",
              "content": {
                "type": "SYMBOL",
                "name": "_node"
              }
            },
            {
              "type": "STRING",
              "value": "\u0000"
            }
          ]
        }
      ]
    },
    "_node": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "text"
        },
        {
          "type": "SYMBOL",
          "name": "tag"
        }
      ]
    },
    "tag": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@"
        },
        {
          "type": "SYMBOL",
          "name": "tag_name"
        },
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_node"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "text": {
      "type": "PATTERN",
      "value": "([^\\n{}@]+(\\n)?)+"
    },
    "_new_block": {
      "type": "PATTERN",
      "value": "[\\n\\s*]+"
    },
    "tag_name": {
      "type": "PATTERN",
      "value": "[a-zA-Z][\\w_]*"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

