{
  "name": "disseminate",
  "rules": {
    "document": {
      "type": "SEQ",
      "members": [
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "block"
          }
        }
      ]
    },
    "block": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "REPEAT1",
              "content": {
                "type": "SYMBOL",
                "name": "_node"
              }
            },
            {
              "type": "SYMBOL",
              "name": "_new_block"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "REPEAT1",
              "content": {
                "type": "SYMBOL",
                "name": "_node"
              }
            },
            {
              "type": "STRING",
              "value": "\u0000"
            }
          ]
        }
      ]
    },
    "_node": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "text"
        },
        {
          "type": "SYMBOL",
          "name": "tag_verb"
        },
        {
          "type": "SYMBOL",
          "name": "tag"
        },
        {
          "type": "SYMBOL",
          "name": "macro"
        }
      ]
    },
    "tag": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_id"
        },
        {
          "type": "SYMBOL",
          "name": "attributes"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "attributes"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_node"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "_verb_node": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_verb_no_paren"
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "{"
            },
            {
              "type": "SYMBOL",
              "name": "_verb_node"
            },
            {
              "type": "STRING",
              "value": "}"
            }
          ]
        }
      ]
    },
    "tag_verb": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_id_verb"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "attributes"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_verb_node"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "macro": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_id"
        }
      ]
    },
    "attributes": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "attribute"
          }
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "attribute": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "attribute_name"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "="
                },
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "SYMBOL",
                      "name": "attribute_value"
                    },
                    {
                      "type": "SYMBOL",
                      "name": "_quoted_attribute_value"
                    }
                  ]
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "text": {
      "type": "PATTERN",
      "value": "([^\\n{}@]+(\\n)?)+"
    },
    "line": {
      "type": "ALIAS",
      "content": {
        "type": "PATTERN",
        "value": "[^\\n{}@]+"
      },
      "named": true,
      "value": "text"
    },
    "_hspace": {
      "type": "PATTERN",
      "value": "[ \\t\\f]*"
    },
    "_new_block": {
      "type": "PATTERN",
      "value": "[\\n\\s*]+"
    },
    "_id": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@"
        },
        {
          "type": "SYMBOL",
          "name": "name"
        }
      ]
    },
    "_id_verb": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "v"
            },
            {
              "type": "STRING",
              "value": "eq"
            }
          ]
        }
      ]
    },
    "name": {
      "type": "PATTERN",
      "value": "[a-zA-Z][\\w_]*"
    },
    "attribute_name": {
      "type": "PATTERN",
      "value": "[A-Za-z]+"
    },
    "attribute_value": {
      "type": "PATTERN",
      "value": "\\w+"
    },
    "_quoted_attribute_value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "'"
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "ALIAS",
                  "content": {
                    "type": "PATTERN",
                    "value": "[^']+"
                  },
                  "named": true,
                  "value": "attribute_value"
                },
                {
                  "type": "BLANK"
                }
              ]
            },
            {
              "type": "STRING",
              "value": "'"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "\""
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "ALIAS",
                  "content": {
                    "type": "PATTERN",
                    "value": "[^\"]+"
                  },
                  "named": true,
                  "value": "attribute_value"
                },
                {
                  "type": "BLANK"
                }
              ]
            },
            {
              "type": "STRING",
              "value": "\""
            }
          ]
        }
      ]
    },
    "_verb_no_paren": {
      "type": "PATTERN",
      "value": "[^\\{\\}]+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

