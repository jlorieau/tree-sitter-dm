{
  "name": "disseminate",
  "rules": {
    "document": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_node"
      }
    },
    "_node": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "tag"
        },
        {
          "type": "FIELD",
          "name": "text",
          "content": {
            "type": "SYMBOL",
            "name": "text"
          }
        }
      ]
    },
    "tag": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_start_tag"
        },
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_node"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "_start_tag": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@"
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "tag_name"
          }
        }
      ]
    },
    "tag_name": {
      "type": "PATTERN",
      "value": "\\w+"
    },
    "text": {
      "type": "PATTERN",
      "value": "[^{}&\\s@]([^{}&@]*[^{}&@\\s])?"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

